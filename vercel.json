{
    "version": 2,
    "functions": {
        "api/*.php": { "runtime": "vercel-php@0.6.0" }
    },
    "routes": [
        { 
            "src": "/css/filament/filament/app.css",
            "dest": "/public/css/filament/filament/app.css"
        },
        { 
            "src": "/css/filament/forms/forms.css",
            "dest": "/public/css/filament/forms/forms.css"
        },
        { 
            "src": "/css/filament/support/support.css",
            "dest": "/public/css/filament/support/support.css"
        },
        { 
            "src": "/js/filament/filament/app.js",
            "dest": "/public/js/filament/filament/app.js"
        },
        { 
            "src": "/js/filament/filament/echo.js",
            "dest": "/public/js/filament/filament/echo.js"
        },
        { 
            "src": "/js/filament/forms/components/color-picker.js",
            "dest": "/public/js/filament/forms/components/color-picker.js"
        },
        { 
            "src": "/js/filament/forms/components/date-picker.js",
            "dest": "/public/js/filament/forms/components/color-picker.js"
        },
        { 
            "src": "/js/filament/forms/components/file-upload.js",
            "dest": "/public/js/filament/forms/components/file-upload.js"
        },
        { 
            "src": "/js/filament/forms/components/key-value.js",
            "dest": "/public/js/filament/forms/components/key-value.js"
        },
        { 
            "src": "/js/filament/forms/components/markdown-editor.js",
            "dest": "/public/js/filament/forms/components/markdown-editor.js"
        },
        { 
            "src": "/js/filament/forms/components/rich-editor.js",
            "dest": "/public/js/filament/forms/components/rich-editor.js"
        },
        { 
            "src": "/js/filament/forms/components/select.js",
            "dest": "/public/js/filament/forms/components/select.js"
        },
        { 
            "src": "/js/filament/forms/components/select.js",
            "dest": "/public/js/filament/forms/components/select.js"
        },
        { 
            "src": "/js/filament/forms/components/tags-input.js",
            "dest": "/public/js/filament/forms/components/tags-input.js"
        },
        { 
            "src": "/js/filament/forms/components/textarea.js",
            "dest": "/public/js/filament/forms/components/textarea.js"
        },
        { 
            "src": "/js/filament/tables/components/tables.js",
            "dest": "/public/js/filament/tables/components/tables.js"
        },
        { 
            "src": "/js/filament/widgets/components/stats-overview/stat/chart.js",
            "dest": "/public/js/filament/widgets/components/stats-overview/stat/chart.js"
        },
        { 
            "src": "/js/filament/widgets/components/chart.js",
            "dest": "/public/js/filament/widgets/components/chart.js"
        },
        { 
            "src": "/js/filament/notifications/notifications.js",
            "dest": "/public/js/filament/notifications/notifications.js"
        },
        { 
            "src": "/js/filament/support/async-alpine.js",
            "dest": "/public/js/filament/support/async-alpine.js"
        },
        { 
            "src": "/js/filament/support/support.js",
            "dest": "/public/js/filament/support/support.js"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ],
    "env": {
        "APP_ENV": "production",
        "APP_DEBUG": "false",

        "APP_CONFIG_CACHE": "/tmp/config.php",
        "APP_EVENTS_CACHE": "/tmp/events.php",
        "APP_PACKAGES_CACHE": "/tmp/packages.php",
        "APP_ROUTES_CACHE": "/tmp/routes.php",
        "APP_SERVICES_CACHE": "/tmp/services.php",
        "VIEW_COMPILED_PATH": "/tmp",

        "CACHE_DRIVER": "array",
        "LOG_CHANNEL": "stderr",
        "SESSION_DRIVER": "cookie"
    }
}
